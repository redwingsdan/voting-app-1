    <script src="~/Scripts/d3/d3.min.js"></script>
    <script src="~/Scripts/react/react-0.12.2.min.js"></script>

    <script src="~/Scripts/jsx/piechart.jsx"></script>
    <script src="~/Scripts/jsx/dashboard.jsx"></script>

}